{
  "name": "@tec/business",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.js"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0 --fix",
    "build": "rimraf dist && tsc && tsc-alias -p tsconfig.aliases.json",
    "watch": "concurrently --kill-others \"tsc -w\" \"tsc-alias -w\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate": "turbo gen business"
  },
  "devDependencies": {
    "@tec/eslint-config": "*",
    "@tec/typescript-config": "*",
    "@turbo/gen": "^2.2.3",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.1.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3"
  }
}
