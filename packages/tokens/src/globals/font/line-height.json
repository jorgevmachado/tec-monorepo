{
  "font": {
    "lineHeight": {
      "$type": "lineHeight",
      "tiny": {
        "$value": "{size.x-regular}",
        "$comment": "1.125rem => 18px if 16px = 1rem"
      },
      "x-small": {
        "$value": "{size.xx-regular}",
        "$comment": "1.25rem => 20px if 16px = 1rem"
      },
      "small": {
        "$value": "{size.medium}",
        "$comment": "1.5rem => 24px if 16px = 1rem"
      },
      "regular": {
        "$value": "{size.xx-medium}",
        "$comment": "1.75rem => 28px if 16px = 1rem"
      },
      "medium": {
        "$value": "{size.large}",
        "$comment": "2rem => 32px if 16px = 1rem"
      },
      "large": {
        "$value": "{size.xx-large}",
        "$comment": "2.5rem => 40px if 16px = 1rem"
      },
      "x-large": {
        "$value": "{size.xx-big}",
        "$comment": "3.25rem => 52px if 16px = 1rem"
      },
      "big": {
        "$value": "{size.huge}",
        "$comment": "4rem => 64px if 16px = 1rem"
      },
      "huge": {
        "$value": "{size.xx-huge}",
        "$comment": "4.5rem => 72px if 16px = 1rem"
      },
      "giant": {
        "$value": "{size.giant}",
        "$comment": "5rem => 80px if 16px = 1rem"
      }
    }
  }
}